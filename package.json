{
  "name": "@jato/auto-scripts",
  "version": "28.1.0",
  "description": "Azure DevOps build CLI",
  "scripts": {
    "postinstall": "npm run build",
    "dev": "npm run build && node dist/_auto.js conf-env",
    "build": "rimraf dist && tsc -p tsconfig.app.json",
    "test": "jest --collectCoverage",
    "lint": "eslint . -c ./eslint.config.mjs",
    "doc": "typedoc --tsconfig tsconfig.app.json"
  },
  "bin": {
    "auto": "./dist/_auto.js",
    "auto-build-templates": "./dist/buildTemplates.js",
    "auto-bump-major": "./dist/bump.js 1",
    "auto-bump-minor": "./dist/bump.js 2",
    "auto-bump-patch": "./dist/bump.js 3",
    "auto-pub": "./dist/pub.js",
    "auto-pub-major": "./dist/pub.js 1",
    "auto-pub-minor": "./dist/pub.js 2",
    "auto-pub-patch": "./dist/pub.js 3",
    "auto-clean-config": "./dist/cleanConfig.js",
    "auto-conf-to-local": "./dist/configToLocalSettings.js",
    "auto-conf-sort": "./dist/configSort.js",
    "auto-env-extract": "./dist/extractEnv.js",
    "auto-help": "./dist/helper.js",
    "auto-local-to-conf": "./dist/localSettingsToConfig.js",
    "auto-project-templates-jest": "./dist/projectTemplates.js jest",
    "auto-project-templates-vitest": "./dist/projectTemplates.js vitest",
    "auto-project-templates": "./dist/projectTemplates.js"
  },
  "files": [
    "/dist"
  ],
  "author": "Eduardo Russo",
  "license": "UNLICENCED",
  "devDependencies": {
    "@eslint/json": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "eslint": "^10.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsonc": "^2.21.1",
    "eslint-plugin-n": "^17.24.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-yml": "^3.1.2",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "jest-junit": "^16.0.0",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.17",
    "typedoc-plugin-missing-exports": "^4.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "overrides": {
    "@types/node": "$@types/node",
    "eslint": "$eslint",
    "globals": "$globals",
    "typescript": "$typescript",
    "typedoc": "$typedoc"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.1",
    "commander": "^14.0.3",
    "inquirer-file-selector": "^1.0.1"
  }
}
