{
  "name": "@jato/auto-scripts",
  "version": "28.1.0",
  "description": "Azure DevOps build CLI",
  "scripts": {
    "postinstall-disabled": "npm run build",
    "dev": "npm run build && node dist/_auto.js conf-env",
    "rollup": "rimraf dist && rollup -c rollup.config.mjs",
    "build": "rimraf dist && tsc -p tsconfig.lib.json",
    "build:lint": "node ./src/eslint/rollup/run.mjs",
    "rename": "renamer --help",
    "test": "jest --collectCoverage",
    "lint": "eslint . -c ./eslint.config.mjs",
    "doc": "typedoc --tsconfig tsconfig.app.json"
  },
  "bin": {
    "auto": "./dist/_auto.js"
  },
  "files": [
    "/dist"
  ],
  "author": "Eduardo Russo",
  "license": "UNLICENCED",
  "devDependencies": {
    "@eslint/json": "^1.0.1",
    "@eslint/markdown": "^7.5.1",
    "@rollup/plugin-eslint": "^9.2.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/cli-progress": "^3.11.6",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@web/rollup-plugin-copy": "^0.5.1",
    "esbuild": "^0.27.3",
    "eslint": "^10.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsonc": "^2.21.1",
    "eslint-plugin-n": "^17.24.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-unicorn": "^63.0.0",
    "eslint-plugin-yml": "^3.2.1",
    "globals": "latest",
    "jest": "^30.2.0",
    "jest-junit": "^16.0.0",
    "rimraf": "^6.1.3",
    "rollup": "^4.57.1",
    "rollup-plugin-rename": "^1.0.1",
    "rollup-plugin-shell": "^1.0.9",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.17",
    "typedoc-plugin-missing-exports": "^4.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  },
  "overrides": {
    "@types/node": "$@types/node",
    "eslint": "$eslint",
    "globals": "$globals",
    "minimatch": "latest",
    "typescript": "$typescript",
    "typedoc": "$typedoc"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.1",
    "ansi-colors": "^4.1.3",
    "cli-progress": "^3.12.0",
    "commander": "^14.0.3",
    "inquirer-file-selector": "^1.0.1"
  }
}
